<%- include('../partials/header') %>
<main id="editdealdetail">

<form action="/deals" class="go-back">
    <button id="dealbackhomefmedit">Back to Deals</button>
</form>

<form id ="dealeditform" action="/deals/<%= deal._id %>?_method=PUT" method="POST" autocomplete="off">
    <label>Deal/Partnership Name:</label>
    <input type="text" name="name" value="<%= deal.name %>">
    <label>Details:</label>
    <textarea id="dealdetailsbox" size="50" name="info" value="<%= deal.info %>"></textarea>
    <button type="submit">Update Deal/Partnership</button>
</form>

</main>
<%- include('../partials/footer') %>